{
  "name": "geraforte",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "fetch-pre-build-content": "node ./config/pre-info-before-build.js",
    "predev": "npm run fetch-pre-build-content",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider next dev",
    "prebuild": "npm run fetch-pre-build-content",
    "build": "NODE_OPTIONS=--openssl-legacy-provider next build",
    "prebuild:export": "npm run fetch-pre-build-content",
    "build:export": "next build && next export",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "generate": "npm run plop --plopfile generators/plopfile.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "npm run lint --fix",
      "npm run test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/color": "^3.0.1",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-slick": "^0.23.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.21.1",
    "color": "^3.1.3",
    "dotenv": "^16.0.1",
    "formik": "^2.2.8",
    "git-commit-msg-linter": "^3.0.0",
    "google-map-react": "^2.1.9",
    "graphql-request": "^2.0.0",
    "next": "^10.0.6",
    "next-i18next": "^8.2.0",
    "next-pwa": "^3.1.4",
    "react": "^16.13.1",
    "react-bnb-gallery": "^1.4.4",
    "react-dom": "16.13.1",
    "react-google-recaptcha": "^2.1.0",
    "react-material-ui-carousel": "^2.2.1",
    "react-slick": "^0.28.0",
    "react-social-icons": "^4.1.0",
    "react-youtube": "^7.13.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1",
    "webpack": "^4.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/google-map-react": "^2.1.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^20.5.3",
    "@types/react": "18.0.1",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.3.0",
    "plop": "^2.7.4",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.1"
  },
  "overrides": {
    "node-sass": "^9.0.0"  
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
